					color #fff
					background #972483
				// .child-menu
				// 	display block
			a
				float left
				width 100%
				height 50px
				line-height 50px
				font-size 18px
				color #333
			.child-menu
				display none
				position absolute
				top 50px
				z-index 10
				left 0
				right 0
				padding 40px 0
				background #972483
